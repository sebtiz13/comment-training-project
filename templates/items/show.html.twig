{% extends 'base.html.twig' %}

{% block title %}{{ item.title }}{% endblock %}

{% block body %}
  <div class="container">
    <img
      alt="{{ item.title }}"
      src="{{ item.image }}"
      style="object-fit: cover; width: 100%; height: 500px;"
    >
    <h1>{{ item.title }}</h1>
  </div>
{% endblock %}
